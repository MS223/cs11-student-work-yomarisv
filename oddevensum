list = [3,4,7,13,54,32,653,256,1,41,65,83,92,31]
def odd_sum(input):
   total=0
   for x in input:
       if x % 2 == 1:
           total=total + x
   print(total)
odd_sum(list)
def even_sum(input):
   total=0
   for x in input:
       if x % 2 == 0:
           total=total + x
   print(total)
even_sum(list)
